				<dict>
					<key>COMMAND_STRING</key>
					<string>python3 -c "
import os
import subprocess
import sys
from pathlib import Path

def main():
    input_directory = sys.argv[1]
    output_directory = sys.argv[2]
    
    allowed_extensions = {'.md', '.csv', '.xls'}

    for root, _, files in os.walk(input_directory):
        for file in files:
            _, ext = os.path.splitext(file)

            if ext in allowed_extensions:
                input_file_path = os.path.join(root, file)
                rel_path = os.path.relpath(root, input_directory)
                new_directory = os.path.join(output_directory, rel_path)
                os.makedirs(new_directory, exist_ok=True)
                output_file_path = os.path.join(new_directory, os.path.splitext(file)[0] + '.mdx')

                if ext == '.md':
                    with open(input_file_path, 'r') as input_file, open(output_file_path, 'w') as output_file:
                        output_file.write(input_file.read())
                else:
                    pandoc_cmd = f'pandoc \"{input_file_path}\" -t markdown -o \"{output_file_path}\"'
                    subprocess.run(pandoc_cmd, shell=True, check=True)

if __name__ == '__main__':
    main()
" "$@"






